<?xml version="1.0" encoding="utf-8" ?>
<Defs>
 <!-- Archotech nose -->
  
    <HediffDef ParentName="AddedBodyPartBase">
    <defName>CarnapArchotechnose</defName>
    <label>archotech nose</label>
    <labelNoun>an archotech nose</labelNoun>
    <description>An installed archotech nose.</description>
    <descriptionHyperlinks><ThingDef>CarnapArchotechnose</ThingDef></descriptionHyperlinks>
    <spawnThingOnRemoved>CarnapArchotechnose</spawnThingOnRemoved>
    <addedPartProps>
      <solid>true</solid>
<!-- Efficiency -->
      <partEfficiency>2.00</partEfficiency>  
      <betterThanNatural>true</betterThanNatural>
    </addedPartProps>
    <stages>
			<li>
				<capMods>
					<li>
						<capacity>Breathing</capacity>
						<offset>0.40</offset>
					</li>
				</capMods>
			</li>
		</stages>
  </HediffDef>

  <ThingDef ParentName="BodyPartArchotechBase">
    <defName>CarnapArchotechnose</defName>
    <label>archotech nose</label>
<!-- Description -->
    <description>An advanced artificial nose built by archotech. It significantly enhhances pawn's air pumping capacities and can even take lung functions if lungs are destroyed. Even if it is harmed, it repairs itself over time. Its internal workings are a mystery to all human minds.</description> 
    <descriptionHyperlinks><RecipeDef>InstallCarnapArchotechnose</RecipeDef></descriptionHyperlinks>
    <statBases>
 <!-- Cost, mass, reward (RewardStandardHighFreq; RewardStandardMidFreq; RewardStandardLowFreq) -->
      <MarketValue>3200</MarketValue> 
      <Mass>1</Mass>
    </statBases>
     <thingSetMakerTags>
      <li>RewardStandardHighFreq</li>
    </thingSetMakerTags>
  </ThingDef>

  <RecipeDef ParentName="SurgeryInstallBodyPartArtificialBase">
    <defName>InstallCarnapArchotechnose</defName>
    <label>install archotech nose</label>
    <description>Install an archotech nose.</description>
    <descriptionHyperlinks>
      <ThingDef>CarnapArchotechnose</ThingDef>
      <HediffDef>CarnapArchotechnose</HediffDef>
    </descriptionHyperlinks>
    <jobString>Installing archotech nose.</jobString>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>CarnapArchotechnose</li>
          </thingDefs>
        </filter>
        <count>1</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>CarnapArchotechnose</li>
      </thingDefs>
    </fixedIngredientFilter>
    <appliedOnFixedBodyParts>
<!-- Fix capitalization -->
      <li>Nose</li>
    </appliedOnFixedBodyParts>
    <addsHediff>CarnapArchotechnose</addsHediff>
  </RecipeDef>


